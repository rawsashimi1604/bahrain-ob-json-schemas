    },
    "AccountType": {
      "$ref": "OBExternalAccountTypeCode.json"
    },
    "AccountSubType": {
      "$ref": "OBExternalAccountSubTypeCode.json"
    },
    "Description": {
      "$ref": "Description.json"
    },
    "Nickname": {
      "$ref": "Nickname.json"
